double median(vector<int>& a, vector<int>& b) {
	// Write your code here.
	vector<int> m;
       
        m.insert(m.begin(), a.begin(), a.end());
        m.insert(m.end(), b.begin(), b.end());
        
        sort(m.begin(),m.end());
        
        return (m[m.size()/2]+m[(m.size()-1)/2])/2.00;
}
